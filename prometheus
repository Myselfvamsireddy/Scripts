#zip link to get prometheus
wget https://github.com/prometheus/prometheus/releases/download/v2.30.0/prometheus-2.30.0.linux-amd64.tar.gz
# to unzip 
tar -xzf prometheus-2.30.0.linux-amd64.tar.gz
# to rename
mv prometheus-2.30.0.linux-amd64 prometheus
# to enter into the renamed directory
cd prometheus
#similarly we need to run it on background
nohup ./prometheus &
